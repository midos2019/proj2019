{% extends 'base.html.twig' %}

{% block title %}Institution{% endblock %}

{% block body %}
    <h1>Institution</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ institution.id }}</td>
            </tr>
            <tr>
                <th>Ministry</th>
                <td>{{ institution.ministry }}</td>
            </tr>
            <tr>
                <th>Office</th>
                <td>{{ institution.office }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ institution.name }}</td>
            </tr>
            <tr>
                <th>Director</th>
                <td>{{ institution.director }}</td>
            </tr>
            <tr>
                <th>Economist</th>
                <td>{{ institution.economist }}</td>
            </tr>
            <tr>
                <th>Administrator</th>
                <td>{{ institution.administrator }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ institution.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ institution.city }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ institution.phone }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ institution.fax }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('institution_index') }}">back to list</a>

    <a href="{{ path('institution_edit', {'id': institution.id}) }}">edit</a>

    {{ include('institution/_delete_form.html.twig') }}
{% endblock %}
